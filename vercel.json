{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "maximumFileSizeToCacheInBytes": 10485760
      }
    }
  ],
  "framework": "sveltekit",
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "outputDirectory": "build",
  "nodeVersion": "18.x"
} 